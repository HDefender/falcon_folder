public class Main {
    public static void main(String[] args) {
        counterService Gas = new counterService("Gas", 0, "m3");
        counterService coldWater = new counterService("Cold water", 10, "m3");
        counterService hotWater = new counterService("Hot water", 20, "m3");
        counterService Electro = new counterService("Electricity", 30, "kWh");


        Gas.setCounter(70);
        Electro.incrimentOne();
        coldWater.getName();
        Gas.getCounter();
        hotWater.setZero();

        }
}
